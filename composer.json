{
  "name": "bowlofsoup/couchbase-migrations-bundle",
  "description": "Migrations for Couchbase in a Symfony bundle.",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "symfony/console": "3.4.*",
    "symfony/symfony": "3.4.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.10",
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": { "BowlOfSoup\\CouchbaseMigrationsBundle\\": "src" }
  },
  "dev-autoload": {
    "psr-4": { "BowlOfSoup\\CouchbaseMigrationsBundle\\Tests\\": "tests" }
  }
}
