{
  "name": "bowlofsoup/couchbase-migrations-bundle",
  "description": "Migrations for Couchbase in a Symfony bundle.",
  "license": "MIT",
  "require": {
    "ext-couchbase": "*",
    "php": ">=8.3",
    "symfony/console": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/process": "^7.0",
    "symfony/config": "^7.0",
    "symfony/http-kernel": "^7.0",
    "symfony/yaml": "^7.0",
    "couchbase/couchbase": "^4.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.56",
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "2.0.x-dev"
  },
  "autoload": {
    "psr-4": { "BowlOfSoup\\CouchbaseMigrationsBundle\\": "src" }
  },
  "autoload-dev": {
    "psr-4": { "BowlOfSoup\\CouchbaseMigrationsBundle\\Tests\\": "tests" }
  }
}
